kmousetoolui.cpp: $(srcdir)/kmousetoolui.ui kmousetoolui.h kmousetoolui.moc
	rm -f kmousetoolui.cpp
	echo '#include <kdialog.h>' > kmousetoolui.cpp
	echo '#include <klocale.h>' >> kmousetoolui.cpp
	$(UIC) -tr ${UIC_TR} -i kmousetoolui.h $(srcdir)/kmousetoolui.ui > kmousetoolui.cpp.temp ; ret=$$?; \
	$(PERL) -pe "s,${UIC_TR}( \"\" ),QString::null,g" kmousetoolui.cpp.temp | $(PERL) -pe "s,${UIC_TR}( \"\"\, \"\" ),QString::null,g" | $(PERL) -pe "s,image([0-9][0-9]*)_data,img\$$1_kmousetoolui,g" | $(PERL) -pe "s,: QWizard\(,: KWizard(,g" >> kmousetoolui.cpp ;\
	rm -f kmousetoolui.cpp.temp ;\
	if test "$$ret" = 0; then echo '#include "kmousetoolui.moc"' >> kmousetoolui.cpp; else rm -f kmousetoolui.cpp ; exit $$ret ; fi

kmousetoolui.h: $(srcdir)/kmousetoolui.ui
	rm -rf kmousetoolui.h;
	$(UIC) $(srcdir)/kmousetoolui.ui | $(PERL) -pi -e "s,public QWizard,public KWizard,g; s,#include <qwizard.h>,#include <kwizard.h>,g" >> kmousetoolui.h ;
kmousetoolui.moc: kmousetoolui.h
	$(MOC) kmousetoolui.h -o kmousetoolui.moc

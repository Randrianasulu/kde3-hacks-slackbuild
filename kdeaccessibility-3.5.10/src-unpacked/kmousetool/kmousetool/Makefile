kmousetool.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/mtstroke.cpp $(srcdir)/kmousetool.cpp $(srcdir)/main.cpp kmousetoolui.cpp  kmousetoolui.moc kmousetool.moc
	@echo 'creating kmousetool.all_cpp.cpp ...'; \
	rm -f kmousetool.all_cpp.files kmousetool.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> kmousetool.all_cpp.final; \
	for file in mtstroke.cpp kmousetool.cpp main.cpp kmousetoolui.cpp ; do \
	  echo "#include \"$$file\"" >> kmousetool.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> kmousetool.all_cpp.final; \
	done; \
	cat kmousetool.all_cpp.final kmousetool.all_cpp.files > kmousetool.all_cpp.cpp; \
	rm -f kmousetool.all_cpp.final kmousetool.all_cpp.files

